            border-radius: 5px;
        }
        button {
            margin-top: 10px;
            padding: 10px 20px;
            background-color: #0056b3;
            color: white;
            border: none;
            border-radius: 5px;
            cursor: pointer;
        }
        button:hover {
            background-color: #004080;
        }
        .result {
            margin-top: 10px;
            font-weight: bold;
        }
        .correct {
            color: green;
        }
        .incorrect {
            color: red;
        }
    </style>
    <script>
        function checkAnswer(wordId, correctAnswer) {
            const userAnswer = document.getElementById(`answer-${wordId}`).value.trim().toLowerCase();
            const result = document.getElementById(`result-${wordId}`);
            if (userAnswer === correctAnswer.toLowerCase()) {
                result.textContent = "Richtig!";
                result.className = "result correct";
            } else {
                result.textContent = `Falsch! Die richtige Antwort ist: ${correctAnswer}`;
                result.className = "result incorrect";
            }
        }
    </script>
</head>
